<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.example.igonan.mindmapper.Productmapper">

    <select id="mindProductInsert" >
        insert into product(pr_num,pr_name,pr_price,pr_count,pr_memo,pr_dpay,pr_type,pr_gallery) <!-- 테이블과 컬럼명 -->
        values (null,#{param1}, #{param2},#{param3}, #{param4},#{param5}, #{param6},#{param7})
        <!--데이터 순서에 맞춰 파라미터 순서 설정 -->
    </select>
    <select id="mindProductImageInsert" >
        insert into product_image(num,pr_num,img_src)
        values (null,#{param1}, #{param2})

    </select>
</mapper>
